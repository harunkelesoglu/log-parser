{
  "name": "@keles0glu/log-parser",
  "version": "1.0.0",
  "description": "CLI to parse log files",
  "main": "dist/parser.js",
  "bin": {
    "log-parser": "./bin/log-parser"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "tsc",
    "start": "npm build && node dist/parser.js",
    "debug": "ts-node src/parser.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest --config=jest.config.ts",
    "release": "semantic-release"
  },
  "author": "harun.kelesoglu19@gmail.com",
  "license": "MIT",
  "engines": {
    "npm": ">=9.5.1",
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/core": "^18.2.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "commander": "^11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harunkelesoglu/log-parser.git"
  },
  "release": {
    "branches": [
      {
        "name": "master",
        "channel": "latest",
        "release": true
      },
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
